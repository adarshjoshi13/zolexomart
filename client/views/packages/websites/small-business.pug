        // Ensure the page scrolls smoothly to the target with a 40px offset
        $(document).ready(function() {
            $('a[href^="#pack"]').on('click', function(e) {
            e.preventDefault();  // Prevent default anchor behavior
            
            var target = $(this.getAttribute('href'));
            
            // Scroll to the target with an offset of 40px
            $('html, body').animate({
                scrollTop: target.offset().top - 150
            }, 500); // Adjust the time (500ms) to control the speed of the scroll
            });
        });



